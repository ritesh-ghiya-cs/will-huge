  <script type="text/javascript"> 
    function confirmCancel() {
      var isCancel = confirm("Are you sure you wish to cancel?");
      if (isCancel) return true;
        return false;
    }
    
    function confirmObjectChanged() {
      var isObjectChanged = confirm("This will clear all custom field filters. Continue?");
      if (isObjectChanged) return true;
        return false;
    }
    
    function confirmClear() {
      var isClear = confirm("This will clear all data for the behavior you are currently editing. Continue?");
      if (isClear) return true;
        return false;
    }
  </script>
   
