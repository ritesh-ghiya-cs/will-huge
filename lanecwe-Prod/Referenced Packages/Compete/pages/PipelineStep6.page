<apex:page sidebar="false" controller="ePrize_Zemzu.PipelineBehaviorsController" extensions="ePrize_Zemzu.PipelineBehaviorsStep6ControllerExt" tabstyle="ContestBuilder__tab">
  <script type="text/javascript">
    function confirmCancel() {
      var isCancel = confirm("Are you sure you wish to cancel?");
      if (isCancel) return true;
      return false;
    } 
  </script>
  
  <apex:form >
    <apex:pageBlock >
      <p>
      Below is a summary of your contest. Wow, you just made something awesome! After you press Save, your contest will go live with the details entered below.
      </p>
    </apex:pageBlock>
    <apex:pageBlock title="Confirm Contest Details">
      <apex:pageMessages />
      <apex:pageBlockButtons >
        <apex:commandButton action="{!previousStep}" value="Back"/>
        <apex:commandButton action="{!cancel}" value="Cancel" onclick="return confirmCancel()" immediate="true"/>
        <apex:commandButton action="{!save}" value="Save"/>
        <br/>
        <apex:inputCheckbox value="{!sendEmail}" rendered="{!changes}">Send Email of Update? </apex:inputCheckbox>
      </apex:pageBlockButtons> 
      <apex:pageBlockSection title="Contest Information" collapsible="false">
        <apex:outputField id="contestName" value="{!contest.ePrize_Zemzu__ContestName__c}"/>
        <apex:pageBlockSectionItem />
        <apex:pageBlockSectionItem >
          <apex:outputLabel value="Contest Type"/>
          <apex:outputText >{!contest.ePrize_Zemzu__Goal__c}
          </apex:outputText>
        </apex:pageBlockSectionItem>
        <apex:outputField id="contestRankingStyle" value="{!contest.ePrize_Zemzu__RankingStyle__c}"/>
        <apex:outputField id="contestStartDate" value="{!contest.ePrize_Zemzu__StartDate__c}"/>
        <apex:outputField id="contestEndDate" value="{!contest.ePrize_Zemzu__EndDate__c}"/>
        <apex:pageBlockSectionItem rendered="{!contest.ePrize_Zemzu__usingDynamicPoints__c}">
          <apex:outputLabel value="Using Dynamic Points"/>
          <apex:inputCheckbox value="{!contest.ePrize_Zemzu__usingDynamicPoints__c}" disabled="true"></apex:inputCheckbox>
        </apex:pageBlockSectionItem>
        <apex:pageBlockSectionItem rendered="{!contest.ePrize_Zemzu__usingDynamicPoints__c}">
          <apex:outputLabel value="Sum Leaderboard Values By:"/>
          <apex:outputText value="{!pipelineBehaviors[0].pipelineBehaviorEntity.DynamicPointsRefrenceField__c}"></apex:outputText>
        </apex:pageBlockSectionItem>
      </apex:pageBlockSection>

      <apex:pageBlockSection title="Behaviors to Motivate" columns="1" collapsible="false">
