						<apex:outputText value="{!t.ePrize_Zemzu__Comparator__c}" />
					</apex:column>
			
					<apex:column >
						<apex:facet name="header">
						  Value
						</apex:facet>
						<apex:outputText value="{!t.ePrize_Zemzu__CompareToValue__c}" />
