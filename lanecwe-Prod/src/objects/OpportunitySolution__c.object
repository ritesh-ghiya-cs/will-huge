    <enableReports>true</enableReports>
    <fields>
        <fullName>Annual_value__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
AND( 
!( ISPICKVAL( Billing_Frequency__c , &quot;One Time Event&quot;) ),
(Contract_Term__c &gt; 11) 
), 
MRR__c * 12, 
Contract_Value__c 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Annual value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>BidPrice__c</fullName>
        <externalId>false</externalId>
        <label>Sell Price</label>
        <precision>16</precision>
        <required>true</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Billing_Frequency__c</fullName>
        <externalId>false</externalId>
        <label>Billing Freq</label>
        <picklist>
            <picklistValues>
                <fullName>Monthly</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Quarterly</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Annually</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>One Time Event</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Contract_Term__c</fullName>
        <externalId>false</externalId>
        <label>Term</label>
        <precision>2</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contract_Value__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Billing_Frequency__c,&apos;Monthly&apos;),BidPrice__c*Number_of_License__c*Contract_Term__c, 
IF(ISPICKVAL(Billing_Frequency__c,&apos;Quarterly&apos;),((BidPrice__c* Number_of_License__c)/3)* Contract_Term__c, 
IF(ISPICKVAL(Billing_Frequency__c,&apos;Annually&apos;),BidPrice__c* Number_of_License__c*( Contract_Term__c/12),BidPrice__c*Number_of_License__c 
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>TCV</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>MRR__c</fullName>
        <externalId>false</externalId>
        <formula>IF(OR(Contract_Term__c = 0, ISPICKVAL( Billing_Frequency__c ,&apos;One Time Event&apos;)),0, 
(Contract_Value__c / Contract_Term__c) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>MRR</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Number_of_License__c</fullName>
        <externalId>false</externalId>
        <label>Qty</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
