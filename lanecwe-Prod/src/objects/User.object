    <validationRules>
        <fullName>Can_Not_Change_User_Name_For_PGiIntrgrat</fullName>
        <active>true</active>
        <errorConditionFormula>/* DO NOT ever add the override validation rule*/
OR( 
	AND( 
		OR( ISCHANGED(FirstName), ISCHANGED(LastName) ),
		OR( 
			AND( UPPER( PRIORVALUE(FirstName) ) = &quot;PGI&quot;, UPPER( PRIORVALUE(LastName) ) = &quot;INTEGRATION&quot; ), 
			AND( UPPER(FirstName) = &quot;PGI&quot;,	UPPER(LastName) = &quot;INTEGRATION&quot; )
		) 
	), 
	AND( 
		ISNEW() , 
		AND( UPPER(FirstName) = &quot;PGI&quot;,	UPPER(LastName) = &quot;INTEGRATION&quot; )
	) ,
AND(
                    ISCHANGED(Username),
                    LOWER(PRIORVALUE(Username)) = &quot;pgiintegration@pgi.com&quot;
)
)</errorConditionFormula>
        <errorMessage>User&apos;s name or Username cannot be changed.</errorMessage>
    </validationRules>
