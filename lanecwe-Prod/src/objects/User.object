        <active>true</active>
        <description>Will fire an error message if someone tries to save an SMB Acq user, and others eventually without checking the Uses Lead Assignment Rule checkbox.</description>
        <errorConditionFormula>AND(
           CONTAINS( UserRole.Name , &quot;Reps&quot;), 
           CONTAINS( UserRole.Name , &quot;SMB Acq&quot;),
           IsActive = False,
           Uses_Lead_Assignment__c = True
)</errorConditionFormula>
        <errorDisplayField>Uses_Lead_Assignment__c</errorDisplayField>
        <errorMessage>Please ensure this user has been deactivated in the Round Robin Lead assignment rules.  Please uncheck this field in order to save the record and email salesforcecrmsupport@pgi.com to get this resolved ASAP.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_be_non_pgi_com_username</fullName>
        <active>true</active>
        <description>Error if the username does not contain pgi.com.  Needed to enforce SSO and integration processes.</description>
        <errorConditionFormula>AND(
$User.Override_Validation_Rules__c = False,
NOT(
CONTAINS( Username, &quot;pgi.com&quot;))
)</errorConditionFormula>
        <errorDisplayField>Username</errorDisplayField>
        <errorMessage>Please ensure the username ends in pgi.com</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_change_Username_after_creation</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

ISCHANGED(Username), 
!  ISNEW(),
RIGHT(Username, 7) = &quot;pgi.com&quot;
)</errorConditionFormula>
        <errorDisplayField>Username</errorDisplayField>
        <errorMessage>You cannot change the Username as it is part of the integration with Tesla.  If this needs to be updated due to an error, name change, etc. please submit a case that Carmen/Andy will resolve.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_set_to_System_Administrator</fullName>
        <active>true</active>
        <description>Prevents non-admins from creating new system administrators.</description>
        <errorConditionFormula>AND(
ProfileId = &quot;00e30000001rXZs&quot; ||
ProfileId = &quot;00e30000001kms9&quot;, 
NOT(
$User.ProfileId = &quot;00e30000001kms9&quot;
)
)</errorConditionFormula>
        <errorDisplayField>ProfileId</errorDisplayField>
        <errorMessage>You must be a full system administrator to set the profile value to System Administrator</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requires_Manager_when_sales_user_profile</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
           CONTAINS( Profile.Name , &quot;sales&quot;),
           ISNULL(ManagerId)
)</errorConditionFormula>
        <errorDisplayField>ManagerId</errorDisplayField>
        <errorMessage>Please select a manager for this user before you can save.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requires_inactive_if_x_dash_lastname</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
            BEGINS( LastName  , &quot;x-&quot;),
!            IsActive = False
)</errorConditionFormula>
        <errorDisplayField>IsActive</errorDisplayField>
        <errorMessage>Please set this user to be inactive when the last name contains &quot;x-&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requires_xdash_on_last_name_when_inactiv</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
            !   BEGINS( LastName  , &quot;x-&quot;),
            IsActive = False
)</errorConditionFormula>
        <errorDisplayField>LastName</errorDisplayField>
        <errorMessage>Please set the last name to start with &quot;x-&quot; before trying to deactivate a user.  e.g. Blue Pulaski should be Blue x-Pulaski.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>x_Cannot_Create_if_assignment_rule_role</fullName>
        <active>false</active>
        <description>Will fire an error message if someone tries to save an SMB Acq user, and others eventually without checking the Uses Lead Assignment Rule checkbox.</description>
        <errorConditionFormula>AND(
           CONTAINS( UserRole.Name , &quot;Reps&quot;) ||
           CONTAINS( UserRole.Name , &quot;E-Commerce&quot;)||
           CONTAINS( UserRole.Name , &quot;SMB Acq&quot;),
           IsActive = True,
           Uses_Lead_Assignment__c = False
)</errorConditionFormula>
        <errorDisplayField>Uses_Lead_Assignment__c</errorDisplayField>
        <errorMessage>Please ensure this user has been setup in the Round Robin Lead assignment rules.  Please check this field in order to save the record and email salesforcecrmsupport@pgi.com to get this resolved ASAP.</errorMessage>
    </validationRules>
</CustomObject>
