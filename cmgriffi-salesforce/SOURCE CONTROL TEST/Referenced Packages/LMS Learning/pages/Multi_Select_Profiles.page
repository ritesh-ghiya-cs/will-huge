<apex:page standardController="lmscons__Catalog__c" extensions="lmscons.MultiSelectProfiles">
	<apex:includeScript value="{!URLFOR($Resource.lmscons__jquery152min)}" />
	<apex:includeScript value="{!URLFOR($Resource.lmscons__jqueruui1811min)}" />
	<apex:includeScript value="{!URLFOR($Resource.lmscons__jquery_twosidedmultiselect)}" />
<style type="text/css">
.tsmsselect {
			width: 40%;
			float: left;
		}
		
		.tsmsselect select {
			width: 100%;
		}
		
		.tsmsoptions {
			width: 20%;
			float: left;
		}
		
		.tsmsoptions p {
			margin: 2px;
			text-align: center;
			font-size: larger;
			cursor: pointer;
		}
		
		.tsmsoptions p:hover {
			color: White;
			background-color: Silver;
		}
</style>
<apex:form >
	<apex:selectList id="Profiles" value="{!SelectedProfilesList}" size="5" multiselect="true">
        <apex:selectOptions value="{!ProfilesList}"></apex:selectOptions>
    </apex:selectList>
</apex:form>

<script>

$("select[name$=Profiles]").twosidedmultiselect();

</script>


</apex:page>