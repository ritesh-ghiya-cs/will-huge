.ui-widget-overlay {
	background: #010101;
	opacity: 0.6;
	filter:alpha(opacity=60);
	height:100%;
	width:100%;
	z-index: 999;
}
</style>

<script type="text/javascript">
<!------------------ Chatter Javascript Start ----->

	var overlay;
	var lightbox;
	var openedwindow;

	var UploadId = '';
	var newwindow;
	var uploadcontent;
	var i = 0;
	var enableRedir = false;
	var idisSet = false;

	window.onkeypress = handleKeyPress;

	jQuery(document).ready(function(){
		overlay = $('.b-overlay');
		lightbox = $('.b-lightbox');

		jQuery("th:contains('{!$ObjectType.Content_Upload__c.fields.CreatedDate.label}')").text('Upload Date');
		if(getUrlVars()['open_popup_attach']){
			open_popup_attach();
		}
	});

	function init_redir() {
		//alert('enredir');
		uploadcontent = 1;
		i = 1;
		idisSet = true;
		enableRedir = true;
	}

	function handleKeyPress(e) {
		var eventInstance = window.event ? event : e;
		var keyCode = eventInstance.charCode ? eventInstance.charCode : eventInstance.keyCode;
		if (keyCode == 13){

			if (openedwindow == 1) {
				SearchUser();
			}
			else {
				SearchGroup();
			}
			return false;
		}
	}

	function Lightbox() {
		lightbox.fadeIn('fast');
		overlay
			.height( $(document).height() )
			.css('opacity','0.6')
			.fadeIn('fast');
	}


	function Closelightbox() {
		Closelb()


		Refresh();
	}

	function Closelb() {
		lightbox.fadeOut('fast');
		overlay.fadeOut('fast');
	}


	function select_people() {
		openedwindow = 1;
		Lightbox();
	}

	function select_group() {
		openedwindow = 2;
		Lightbox();
	}

	function open_upload_popup() {
		openedwindow = 3;
		Lightbox();
	}

	function open_popup_attach(){
		uploadcontent = 1;
		newwindow = window.open('/apex/Upload_Popup?override=1','name','height=500,width=600');
		if (window.focus) {newwindow.focus()}
	}

	function test_baca() {
		//UploadId = 'a00U0000002biI7';
		//alert(UploadId);
		uploadcontent = 1
		GetUploadedObject(UploadId);
		//redirectWindow();
	}

	function attach_file(){
		Closelb();
		setTimeout('attach2();', 1000);
		}

	function attach2() {
		if (uploadcontent == 1) {
			uploadcontent = 0;
			UploadId = jQuery('input[name$="UploadId"]').val();
			//UploadId
			GetUploadedObject();
		}
		else {
			//window.location.href = window.location.href;//'/apex/MyTraining';
		}
	}

	function getUrlVars() {
		var vars = [], hash;
		var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
		for(var i = 0; i < hashes.length; i++)
		{
			hash = hashes[i].split('=');
			vars.push(hash[0]);
			vars[hash[0]] = hash[1];
		}
		return vars;
	}

	function check_poller() {
		var c = jQuery('[id*="poller_step"]').html();

		if (c == 'true') {
			process_publish();
		}

	}

	function testT() {
		i++;
		var ua = jQuery.browser;
		if( i > 2) {
			if(enableRedir == true) {
				if(idisSet == true) {
					attach_file();
				}
			}
		}
	}


	function closesearch() {
		jQuery("#dialog-modal3").dialog('close');

	}

	function open_sc_popup() {

		var szx = jQuery(window).width()*0.9;
		var szy = jQuery(window).height()*0.9-100;

		jQuery('#dialog-modal3').html('<iframe id="searchcatalogframe" name="searchcatalogframe" frameborder="0" height="'+szy+'px" src="/apex/SearchCatalog" width="100%"></iframe>');

		jQuery('#dialog-modal3').dialog({
			autoOpen: false,
			width: jQuery(window).width()*0.9,
			height: jQuery(window).height()*0.9,
			modal: true,
			resizable: false,
			close: function(event, ui) {
				RefreshPage();
			}
		});


		jQuery("#dialog-modal3").dialog('open');
	}

	function RefreshPage() {
		window.location.reload();
	}


	function init_pg() {

		jQuery('.m-tbl').find('table.m-hide').parent().parent().addClass('m-hide');
		jQuery('.m-tbl').find('table.m-hide').parent().parent().attr('style', 'display: table-row;');
		/*$('.m-tbl').find('table.m-hide tr').addClass('m-faded');*/
		jQuery('.m-tbl').find('table.m-hide').removeClass('m-hide');


		Toggler();
		HideShowToggler();
		Check();
		ShowSession();
		SearchToggler();


		/* last/first child */
		jQuery('.m-tbl th:first-child,.m-tbl td:first-child').addClass('m-first');
		jQuery('.m-tbl th:last-child, .m-tbl td:last-child, .m-first-level tr:last-child').addClass('m-last');
		jQuery('.m-first-level .m-second-level').parent().closest('td.m-first-level').addClass('m-no-bord');

		/* chrome/safari fix */
		jQuery('.b-results .m-tbl tr td:nth-child(2)').addClass('m-auto-width');

		/* ie table fix */
		if (jQuery.browser.msie) {
		jQuery(".m-tbl td:empty, .m-tbl thead th div:empty").html("&nbsp;");
		}

	}

	function change_parent() {
		//jQuery('#searchcatalogframe').html('lalala');
		alert('Trying to change height');


		jQuery('#social_learning').attr('height','1000');
		//jQuery('#social_learning').height = '1000px';
		//jQuery('#social_learning').height = 1000;

	}


</script>

<!------------------ Chatter Javascript End -------------------------->

</head>
<body>

	<div class="b-overlay"></div>

	<apex:actionStatus id="load_select_people" onstop="select_people()">
		<apex:facet name="start">
			<c:EnhancedActionStatus BackColor="#efefef" borderColor="#336699" borderSize="3" height="50px" width="120px" ImageUrl="/img/loading.gif" Message="{!$Label.Consumer_Global_Loading}..." messageStyle="color:darkred;font-size:11pt;font-weight:bold;"/>
		</apex:facet>
	</apex:actionStatus>

	<apex:actionStatus id="load_select_group" onstop="select_group()">
		<apex:facet name="start">
			<c:EnhancedActionStatus BackColor="#efefef" borderColor="#336699" borderSize="3" height="50px" width="120px" ImageUrl="/img/loading.gif" Message="{!$Label.Consumer_Global_Loading}..." messageStyle="color:darkred;font-size:11pt;font-weight:bold;"/>
		</apex:facet>
	</apex:actionStatus>

	<apex:actionStatus id="load_upload_popup" onstop="open_upload_popup()">
		<apex:facet name="start">
			<c:EnhancedActionStatus BackColor="#efefef" borderColor="#336699" borderSize="3" height="50px" width="120px" ImageUrl="/img/loading.gif" Message="{!$Label.Consumer_Global_Loading}..." messageStyle="color:darkred;font-size:11pt;font-weight:bold;"/>
		</apex:facet>
	</apex:actionStatus>


	<apex:actionStatus id="loading">
		<apex:facet name="start">
			<c:EnhancedActionStatus BackColor="#efefef" borderColor="#336699" borderSize="3" height="50px" width="120px" ImageUrl="/img/loading.gif" Message="{!$Label.Consumer_Global_Loading}..." messageStyle="color:darkred;font-size:11pt;font-weight:bold;"/>
		</apex:facet>
	</apex:actionStatus>


<div class="h-base">
	<div class="l-header">
		<h1 class="b-title"><img src="{!URLFOR($Resource.LMSSource, 'i/header-icon.png')}" />{!$Label.MyTraining_title_My_Training}</h1>

		<div class="b-btns">
			<!-- <div class="m-calendar"><a class="b-btn" href="#"><span class="b-icon">&nbsp;</span>Training Calendar</a></div> -->
			<apex:outputPanel rendered="{!lmsConfig.lmscons__Enable_Catalog_Search__c}">
				<div class="m-search">
					<a class="b-btn" href="Javascript:open_sc_popup();"><span class="b-icon">&nbsp;</span>{!$Label.MyTraining_SearchCatalog}</a>
				</div>
			</apex:outputPanel>
		</div>
	</div>

	<apex:pageMessages id="messages" />

<!--  <input type="button" value="par" onclick="change_parent()" /> -->

	<apex:outputpanel layout="none" rendered="{!lmsConfig.lmscons__Social_Learning__c = true}">
		<iframe id="social_learning" name="social_learning" style="z-index: 1;" frameborder="0" height="1px" src="/apex/AddConsumerUsersBatchable" width="100%"></iframe>
	</apex:outputpanel>

	<div class="l-content">

	<apex:outputpanel layout="none" id="mycontent">

		<div class="b-tabs" style="z-index: 0;">
			<div class="b-tab">Current Training</div>
			<!-- <div class="b-tab">Historical Imports</div> -->
			<div class="b-setting" style="z-index: 0;">
				<div class="b-checkbox" style="z-index: 0;">
					<input type="checkbox" checked="checked" id="a" /><label for="a">Show Completed Training</label>
				</div>
				<!--<div class="b-xls-print">
					<a href="#" class="m-xls">excel</a>
					<a href="#" class="m-print">print</a>
				</div>-->
			</div>
		</div>

		<div class="b-tab-bodies">
			<div class="b-tab-body">
				<apex:outputPanel rendered="{!TrainingList.size==0}">
					<table width="100%" border="0" class="m-tbl">
						<tr>
							<td align="center">
								{!$Label.MyTraining_No_Training_to_Complete}
							</td>
						</tr>
					</table>
				</apex:outputPanel>
				<apex:dataTable rendered="{!TrainingList.size!=0}" headerClass="t-head-top" value="{!TrainingList}" var="c" width="100%" border="0" styleClass="m-tbl">

					<!-- ------- -->
					<apex:column headerClass="col1" styleClass="emptyFix m-col1 {!IF(ISNULL(c.TitleCurriculum), '', 'm-plus')}" rendered="{!IF(ISNULL(c.TitleCurriculum), true, false)}">
						<apex:facet name="header" >
							TRAINING
						</apex:facet>
					</apex:column>
					<apex:column headerClass="col2" rendered="{!IF(lmsConfig.lmscons__Show_Type__c, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col2">
						<apex:facet name="header" >
							TRAINING TYPE
						</apex:facet>
					</apex:column>
					<apex:column headerClass="col3" rendered="{!IF(lmsConfig.lmscons__Show_status__c, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col3">
						<apex:facet name="header" >
							STATUS
						</apex:facet>
					</apex:column>
					<apex:column headerClass="col4" rendered="{!IF(lmsConfig.lmscons__Show_progress_bar__c, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col4">
						<apex:facet name="header" >
							PROGRESS
						</apex:facet>
					</apex:column>
					<apex:column headerClass="col5" rendered="{!IF(lmsConfig.lmscons__Show_Score__c, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col5">
						<apex:facet name="header" >
							SCORE
						</apex:facet>
					</apex:column>
					<apex:column headerClass="col6" rendered="{!IF(lmsConfig.lmscons__Show_Duration__c, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col6">
						<apex:facet name="header" >
							DURATION
						</apex:facet>
					</apex:column>
					<apex:column headerClass="col7" rendered="{!IF(lmsConfig.lmscons__Show_Due_Date__c, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col7">
						<apex:facet name="header" >
							DUE DATE
						</apex:facet>
					</apex:column>
					<apex:column headerClass="col8" rendered="{!IF(ISNULL(c.TitleCurriculum), true, false)}" styleClass="emptyFix m-col8">
						&nbsp;
					</apex:column>
					<apex:column headerClass="col9" rendered="{!IF(lmsConfig.lmscons__Enable_certificate__c, IF(ISNULL(c.TitleCurriculum), true, false), false)}" styleClass="emptyFix m-col9">
						&nbsp;
					</apex:column>
					<!-- ------- -->
					<apex:column styleClass="m-col1 {!IF(ISNULL(c.TitleCurriculum), '', 'm-plus')}" rendered="{!IF(ISNULL(c.TitleCurriculum), false, true)}">
						<apex:facet name="header" >
							TRAINING
						</apex:facet>
						<apex:outputText value="{!c.TitleCurriculum}"  />
					</apex:column>
					<apex:column styleClass="m-col2"  rendered="{!IF(lmsConfig.lmscons__Show_Type__c, IF(ISNULL(c.TitleCurriculum), false, true), false)}">
						<apex:facet name="header" >
							TRAINING TYPE
						</apex:facet>
						<apex:outputText value="{!c.Type}"/>
					</apex:column>
					<apex:column styleClass="m-col3" rendered="{!IF(lmsConfig.lmscons__Show_status__c, IF(ISNULL(c.TitleCurriculum), false, true), false)}">
						<apex:facet name="header" >
							STATUS
						</apex:facet>
						<apex:outputField value="{!c.CItem.Curriculum_Assignment__r.lmscons__Status__c}"/>
					</apex:column>
					<apex:column styleClass="m-col4" rendered="{!IF(lmsConfig.lmscons__Show_progress_bar__c, IF(ISNULL(c.TitleCurriculum), false, true), false)}">
						<apex:facet name="header" >
							PROGRESS
						</apex:facet>
						<c:ProgressBar percent="{!IF(ISNULL(c.CItem.Curriculum_Assignment__r.lmscons__Curriculum_Progress__c), 0,ROUND(c.CItem.Curriculum_Assignment__r.lmscons__Curriculum_Progress__c,0))}"  ispath="true" score="" />
					</apex:column>
					<apex:column styleClass="m-col5" rendered="{!IF(lmsConfig.lmscons__Show_Score__c, IF(ISNULL(c.TitleCurriculum), false, true), false)}">
						<apex:facet name="header" >
							SCORE
						</apex:facet>
						<!-- Curriculum -->
						<apex:outputText value="{!c.scoreCurriculum}" styleClass="m-bold" />
					</apex:column>
					<apex:column styleClass="m-col6" rendered="{!IF(lmsConfig.lmscons__Show_Duration__c,IF(ISNULL(c.TitleCurriculum), false, true), false)}">
						<apex:facet name="header" >
							DURATION
						</apex:facet>
						<apex:outputText value="{!c.Curr_Duration}"/>
					</apex:column>
					<apex:column styleClass="m-col7" rendered="{!IF(lmsConfig.lmscons__Show_Due_Date__c, IF(ISNULL(c.TitleCurriculum), false, true), false)}">
						<apex:facet name="header" >
							DUE DATE
						</apex:facet>
						<apex:outputField value="{!c.CItem.Curriculum_Assignment__r.lmscons__Due_Date__c}"/>
					</apex:column>
					<apex:column styleClass="m-col8" rendered="{!IF(ISNULL(c.TitleCurriculum), false, true)}">
						&nbsp;
					</apex:column>
					<apex:column styleClass="m-col9" rendered="{!IF(lmsConfig.lmscons__Enable_certificate__c,IF(ISNULL(c.TitleCurriculum), false, true), false)}">
						<apex:outputPanel rendered="{!lmsConfig.lmscons__Enable_certificate__c}">
							<apex:outputLink rendered="{!IF(c.CItem.Curriculum_Assignment__r.lmscons__Curriculum_Progress__c==100.0, true, false)}" value="/apex/{!IF(lmsConfig.lmscons__Certificate_Page_Name__c=='','LMS_Certificate','c__'+lmsConfig.lmscons__Certificate_Page_Name__c)}?Id={!c.CItem.Curriculum_Assignment__r.id}" styleClass="btn b-btn" style="text-decoration:none" target="_blank"><span class="m-award">&nbsp;</span></apex:outputLink>
						</apex:outputPanel>
					</apex:column>

					<!-- ---------------- -->

					<!-- first level -->
					<apex:column styleClass="m-hide-tbl m-first-level" colspan="{!Columns}" breakBefore="true">
						<apex:dataTable width="100%" value="{!c.LpEntryList}" var="LP" border="0" styleClass="{!IF(ISNULL(c.TitleCurriculum), '', 'm-hide')}" rendered="{!c.CuSeqList.size == 0}">
							<apex:column styleClass="m-plus m-col1" style="{!IF(ISNULL(c.TitleCurriculum), 'background-position: 8px 17px;', '')}">
								<div class="m-lev-0">
									<apex:outputText value="{!LP.LpItem.Learning_Path_Assignment__r.Learning_Path__r.Name}"/>
								</div>
							</apex:column>
							<apex:column styleClass="m-col2"  rendered="{!lmsConfig.lmscons__Show_Type__c}">
								<apex:outputText value="{!LP.Type}"/>
							</apex:column>
							<apex:column styleClass="m-col3" rendered="{!lmsConfig.lmscons__Show_status__c}">
								<apex:outputField value="{!LP.LpItem.Learning_Path_Assignment__r.lmscons__Status__c}"/>
							</apex:column>
							<apex:column styleClass="m-col4" rendered="{!lmsConfig.lmscons__Show_progress_bar__c}">
								<c:ProgressBar percent="{!IF(ISNULL(LP.LpItem.Learning_Path_Assignment__r.lmscons__Learning_Path_Progress__c),0,ROUND(LP.LpItem.Learning_Path_Assignment__r.lmscons__Learning_Path_Progress__c,0))}"  ispath="false" score="" />
							</apex:column>
							<apex:column styleClass="m-col5" rendered="{!lmsConfig.lmscons__Show_Score__c}">
								<!-- Path -->
								<apex:outputText value="{!LP.scoreLpEntry}" styleClass="m-bold" />
							</apex:column>
							<apex:column styleClass="m-col6" rendered="{!lmsConfig.lmscons__Show_Duration__c}">
								<apex:outputText value="{!LP.LP_Duration}"/>
							</apex:column>
							<apex:column styleClass="m-col7" rendered="{!lmsConfig.lmscons__Show_Due_Date__c}">
								<apex:outputField value="{!LP.LpItem.Learning_Path_Assignment__r.lmscons__Due_Date__c}"/>
							</apex:column>
							<apex:column styleClass="m-col8 m-center">
								&nbsp;
							</apex:column>
							<apex:column styleClass="m-col9 m-center" rendered="{!lmsConfig.lmscons__Enable_certificate__c}">
								<apex:outputPanel rendered="{!lmsConfig.lmscons__Enable_certificate__c}">
									<apex:outputLink rendered="{!IF(LP.LpItem.Learning_Path_Assignment__r.lmscons__Learning_Path_Progress__c==100.0, true, false)}" value="/apex/{!IF(lmsConfig.lmscons__Certificate_Page_Name__c=='','LMS_Certificate','c__'+lmsConfig.lmscons__Certificate_Page_Name__c)}?Id={!LP.LpItem.Learning_Path_Assignment__r.id}" styleClass="btn b-btn" style="text-decoration:none" target="_blank"><span class="m-award">&nbsp;</span></apex:outputLink>
								</apex:outputPanel>
							</apex:column>

							<!-- second level -->
							<apex:column styleClass="m-hide-tbl m-second-level" breakBefore="true" colspan="{!Columns}">
								<!-- sequence -->
								<apex:repeat value="{!LP.LpSeqList}" var="LPS">
									<apex:dataTable width="100%" value="{!LPS.LpEntryObj.MEntryList}" var="M" rowClasses="m-faded"  styleClass="m-hide">
										<apex:column styleClass="m-col1">
											<div class="{!IF(ISBLANK(c.TitleCurriculum), 'm-lev-1', 'm-lev-0')}">
												<span class="m-icon-type-1">&nbsp;</span>
												<apex:outputText value="{!M.MItem.Training_Content__r.lmscons__Title__c}"/>
											</div>
										</apex:column>
										<apex:column styleClass="m-col2" rendered="{!lmsConfig.lmscons__Show_Type__c}">
											<apex:outputText value="{!M.Type}"/>
										</apex:column>
										<apex:column styleClass="m-col3" rendered="{!lmsConfig.lmscons__Show_status__c}">
											<apex:outputField value="{!M.MItem.lmscons__Status__c}"/>
										</apex:column>
										<apex:column styleClass="m-col4" rendered="{!lmsConfig.lmscons__Show_progress_bar__c}">
											<c:ProgressBar percent="{!M.MItem.lmscons__Percent_Complete__c}"  ispath="false" score="" />
										</apex:column>
										<apex:column styleClass="m-col5" rendered="{!lmsConfig.lmscons__Show_Score__c}">
											<apex:outputField value="{!M.MItem.lmscons__Score__c}"/>
										</apex:column>
										<apex:column styleClass="m-col6" rendered="{!lmsConfig.lmscons__Show_Duration__c}">
											<apex:outputText value="{!M.Module_Duration}"/>
										</apex:column>
										<apex:column styleClass="m-col7" rendered="{!lmsConfig.lmscons__Show_Due_Date__c}">
											<apex:outputField value="{!M.MItem.lmscons__Due_Date__c}"/>
										</apex:column>
										<apex:column styleClass="m-col8 m-center">
