<apex:page standardController="OverrideBehaviour__c" sidebar="false" showHeader="false" recordSetVar="acc" >
    <apex:form >
          <apex:pageBlock >
            <apex:pageBlockTable value="{!acc}" var="a">
                <apex:column value="{!a.Name}"/>
                <apex:column value="{!a.Id}"/>
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form>
    
</apex:page>