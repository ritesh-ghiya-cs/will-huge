<apex:page standardController="Customer__c" recordSetVar="user" renderAs="PDF">
    <apex:pageblock >
        <apex:pageblocksection >
            <apex:pageBlockTable value="{!user}" var="con">
            
            <apex:column value="{!con.Last_Name__c}"/>
            <apex:column value="{!con.Gender__c}"/>
            <apex:column value="{!con.City__c}"/>
            </apex:pageBlockTable>
        </apex:pageblocksection>
    </apex:pageblock>
 
</apex:page>