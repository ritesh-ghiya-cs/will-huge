    <validationRules>
        <fullName>rC_Giving__Inactive_Items</fullName>
        <active>true</active>
        <description>This rule states that Items or Packages must be &quot;Active&quot;.</description>
        <errorConditionFormula>AND($Setup.rC_Giving__Opportunity_Product_Validation__c.rC_Giving__Inactive_Items__c, OR( Product2.IsActive  = false , rC_Giving__Item__r.IsActive = false , rC_Giving__Package__r.IsActive = false))</errorConditionFormula>
        <errorMessage>Cannot add Item or Package because it is not Active.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rC_Giving__Individually_Not_Available_Items</fullName>
        <active>true</active>
        <description>This rule states that Items with &quot;Not_available_individually&quot; checked must be part of a package when being added to a Giving.</description>
        <errorConditionFormula>AND($Setup.rC_Giving__Opportunity_Product_Validation__c.rC_Giving__Individually_Not_Available_Items__c, OR(Product2.rC_Giving__Not_available_individually__c,  rC_Giving__Item__r.rC_Giving__Not_available_individually__c ), rC_Giving__Package__c = null)</errorConditionFormula>
        <errorMessage>ERROR : This Item can not be added individually.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rC_Giving__Items_With_Expired_Start_End_Date</fullName>
        <active>true</active>
        <description>This rule states that the &quot;Start Date&quot; and &quot;End Date&quot; of Items being added to a Giving must be before and after (respectively) the Giving &quot;Close Date&quot;.</description>
        <errorConditionFormula>AND($Setup.rC_Giving__Opportunity_Product_Validation__c.rC_Giving__Items_With_Expired_Start_End_Date__c, OR(Opportunity.CloseDate  &gt;   rC_Giving__Item__r.rC_Giving__End_Date__c , Opportunity.CloseDate  &gt;  rC_Giving__Package__r.rC_Giving__End_Date__c  , Opportunity.CloseDate  &gt;  Product2.rC_Giving__End_Date__c , Opportunity.CloseDate  &lt;  rC_Giving__Item__r.rC_Giving__Start_Date__c ,  Opportunity.CloseDate  &lt;  rC_Giving__Package__r.rC_Giving__Start_Date__c , Opportunity.CloseDate  &lt;  Product2.rC_Giving__Start_Date__c ))</errorConditionFormula>
        <errorMessage>Close Date should be between Start and End Date</errorMessage>
    </validationRules>
