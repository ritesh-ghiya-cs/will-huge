<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Custom settings to control the custom batch job for batch upload processing</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Batch_Size__c</fullName>
        <defaultValue>1</defaultValue>
        <description>Batch size of the batch while processing records. Lower number reduces the possibility of a CPU timeout error. Recommended value: 1</description>
        <externalId>false</externalId>
        <inlineHelpText>Batch size of the batch while processing records. Lower number reduces the possibility of a CPU timeout error. Recommended value: 1</inlineHelpText>
        <label>Batch Size</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Limit_Number_of_Records__c</fullName>
        <defaultValue>800</defaultValue>
        <description>Number of records to be picked up in the initial query. Lower number reduces the possibility of a heap size exception. Optimal value: 800</description>
        <externalId>false</externalId>
        <inlineHelpText>Number of records to be picked up in the initial query. Lower number reduces the possibility of a heap size exception. Optimal value: 800</inlineHelpText>
        <label>Limit Number of Records</label>
        <precision>4</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Re_schedule_After_Finish__c</fullName>
        <defaultValue>true</defaultValue>
        <description>If checked, will re-schedule the batch once the existing job finishes</description>
        <externalId>false</externalId>
        <inlineHelpText>If checked, will re-schedule the batch once the existing job finishes</inlineHelpText>
        <label>Re-schedule After Finish</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Subsequent_Batch_Delay__c</fullName>
        <defaultValue>5</defaultValue>
        <description>Delay in minutes before re-scheduling the batch after completing</description>
        <externalId>false</externalId>
        <inlineHelpText>Delay in minutes before re-scheduling the batch after completing</inlineHelpText>
        <label>Subsequent Batch Delay</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Custom Batch Upload Batch Settings</label>
    <visibility>Protected</visibility>
</CustomObject>
