<apex:component layout="none">
<style>
body {
background-image: url("{!$Resource.northstar_backgroundTile}");
}
header {
background-color: #fff;
margin-top: 10px;
padding: 10px 15px 15px 10px;
overflow: hidden;
}
header a {
background-color: #fff;
float: left;
white-space: nowrap;
}
header a img {
margin-right: 10px;
}
header a img:last-child {
    display: none;
}
#navigation {
clear: left;
}
@media (max-width: 687px) {
    #content_section {
        margin-top: -25px;
    }
}
@media (min-width: 992px) {
    header a img:last-child {
        display: inline;
    }
}
</style>
<!-- component: Begin Minnesota Chapter Header -->
<header>
    <a href="http://minnesota.sierraclub.org/">
        <img src="{!$Resource.northstar_header2}" />
        <img src="{!$Resource.northstar_sierraLogo}" />
    </a>
</header>
<!-- component: End Minnesota Chapter Header -->

</apex:component>