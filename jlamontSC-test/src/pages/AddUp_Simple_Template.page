<apex:page >
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <link href='//fonts.googleapis.com/css?family=Oswald:700,300|Open+Sans:400italic,400,700' rel='stylesheet' type='text/css' />

    <!--[if lte IE 8]>
    <apex:stylesheet value="{!$Resource.AddUp_styles_desktop_css}"/>
    <![endif]-->
    <!--[if gt IE 8]><!-->
    <apex:stylesheet value="{!$Resource.AddUp_styles_css}"/>
    <!--<![endif]-->
    <!-- Custom styles for this template -->
    <apex:stylesheet value="{!$Resource.AddUpCSS}"/>
    
    <style>
    body input[type="submit"].btn {
        background-image: none;
        background-color: #e95d1f;
        font-weight: bold;
        font-size: 3rem;
        font-size: 30px;
        color: #872e06;
        margin-top: 5px;
    }
    body input[type="submit"].btn:hover {
        background-color: #872e06;
        color: #e95d1f;
    }
    .hero-signup-form label {
        color: white !important;
    }
    
    </style>
    
</head>
<body>
  <div class="login-section col-12 white">
  <apex:insert name="body"></apex:insert>
  </div>
</body>
</apex:page>