    <validationRules>
        <fullName>Check_All_SSL_Trust_Bit_Turned_Off</fullName>
        <active>true</active>
        <description>Check to see &quot;All SSL Trust Bit Turned Off&quot; is selected as the only option from the multi picklist</description>
        <errorConditionFormula>(INCLUDES (Trust_Bits__c, &quot;All SSL Trust Bit Turned Off&quot;)) &amp;&amp; 
((INCLUDES (Trust_Bits__c, &quot;Code Signing&quot;)) ||
(INCLUDES (Trust_Bits__c, &quot;Email (S/MIME)&quot;)) ||
(INCLUDES (Trust_Bits__c, &quot;Websites (SSL/TLS)&quot;)) ||
(INCLUDES (Trust_Bits__c, &quot;Code&quot;)) ||
(INCLUDES (Trust_Bits__c, &quot;Email&quot;)) ||
(INCLUDES (Trust_Bits__c, &quot;Websites&quot;)))</errorConditionFormula>
        <errorDisplayField>Trust_Bits__c</errorDisplayField>
        <errorMessage>Cannot select another option with &quot;All SSL Trust Bit Turned Off&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Audits_Same_as_Parent</fullName>
        <active>true</active>
        <description>if  &apos;Audits Sames as Parent&apos; field is checked, all the related fields should be blank</description>
        <errorConditionFormula>AND(Audits_Same_as_Parent__c  = TRUE, 
    OR (Standard_Audit__c &lt;&gt; NULL,
        BR_Audit__c &lt;&gt; NULL,
        EV_Audit__c &lt;&gt; NULL,
        NOT (ISPICKVAL(Standard_Audit_Type__c, &quot;&quot;)),
        NOT (ISPICKVAL(BR_Audit_Type__c, &quot;&quot;)),
        NOT (ISPICKVAL(EV_Audit_Type__c, &quot;&quot;)),
        NOT(ISNULL(Standard_Audit_Statement_Date__c)),
        NOT(ISNULL(BR_Audit_Statement_Date__c)),
        NOT(ISNULL(EV_Audit_Statement_Date__c)),
        Auditor__c &lt;&gt; NULL,
        Auditor_Website__c &lt;&gt; NULL, 
        Auditor_Qualifications__c &lt;&gt; NULL   
       )   
    )</errorConditionFormula>
        <errorDisplayField>Audits_Same_as_Parent__c</errorDisplayField>
        <errorMessage>&apos;Audits Same as Parent&apos; is checked. Please uncheck this field or clear rest of the Audit Info fields to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_CP_CPS_Same_as_Parent</fullName>
        <active>true</active>
        <description>if  &apos;CP/CPS Sames as Parent&apos; field is checked, all the related fields should be blank</description>
        <errorConditionFormula>AND(CP_CPS_Same_as_Parent__c  = TRUE, 
     OR( Policy_Documentation__c  &lt;&gt; NULL,
         CA_Document_Repository__c  &lt;&gt; NULL, 
         Certificate_Policy_CP__c  &lt;&gt; NULL,
         Certification_Practice_Statement_CPS__c  &lt;&gt; NULL
       )
    )</errorConditionFormula>
        <errorDisplayField>CP_CPS_Same_as_Parent__c</errorDisplayField>
        <errorMessage>&apos;CP/CPS Same as Parent&apos; is checked. Please uncheck this field or clear rest of the Policies and Practices Information fields to save the record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_PEM_Derived_Fields</fullName>
        <active>true</active>
        <description>If PEM info is provided, do not allow derived fields to be modified.</description>
        <errorConditionFormula>AND (AND (NOT(ISBLANK (X_509_Certificate_PEM__c)), NOT (ISCHANGED(X_509_Certificate_PEM__c))), 
OR (ISCHANGED(Certificate_Issuer_Common_Name__c), 
ISCHANGED(Certificate_Issuer_Organizational_Unit__c), 
ISCHANGED(Certificate_Issuer_Organization__c), 
ISCHANGED(Certificate_Subject_Common_Name__c), 
ISCHANGED(Certificate_Subject_Organization_Unit__c), 
ISCHANGED(Certificate_Subject_Organization__c), 
ISCHANGED(SHA_1_Fingerprint__c), 
ISCHANGED(SHA_256_Fingerprint__c), 
ISCHANGED(Valid_From_GMT__c), 
ISCHANGED(Valid_To_GMT__c), 
ISCHANGED(Certificate_Serial_Number__c), 
ISCHANGED(Basic_Constraints__c), 
ISCHANGED(Key_Usage__c), 
ISCHANGED(Extended_Key_Usage__c), 
ISCHANGED(OCSP_URL_s__c), 
ISCHANGED(CRL_URL_s__c), 
ISCHANGED(Signature_Algorithm__c), 
ISCHANGED(Signing_Key_Parameters__c), 
ISCHANGED(Technically_Constrained__c) 
))</errorConditionFormula>
        <errorMessage>Do not edit fields derived from PEM information. Hit &apos;Cancel&apos; to undo the changes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_cannot_be_Verified_by_CA</fullName>
        <active>true</active>
        <description>A community user cannot choose &apos;Verified&apos; status</description>
        <errorConditionFormula>AND ( $Profile.Name = &apos;Customer Community Plus User - CAs&apos;, 
      ISPICKVAL (Status__c, &apos;Verified&apos;))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>&apos;Verified&apos; status cannot be chosen by a CA</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_SHA_1_Fingerprint_not_Null</fullName>
        <active>true</active>
        <description>Check for SHA-1 Fingerprint to be not null when Type=Root</description>
        <errorConditionFormula>AND ( ISPICKVAL(Type, &apos;Root&apos;), ISBLANK(SHA_1_Fingerprint__c))</errorConditionFormula>
        <errorDisplayField>SHA_1_Fingerprint__c</errorDisplayField>
        <errorMessage>Fill in SHA-1 Fingerprint to save Root Certificate</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fill_Firefox_Release_when_Root_Removed</fullName>
        <active>true</active>
        <description>&apos;Firefox Release When Removed&apos; must be filled in when the root certificate status is &apos;Removed&apos;</description>
        <errorConditionFormula>AND (RecordType.Name = &apos;Root Certificate&apos;, 
ISPICKVAL(Status__c, &apos;Removed&apos;), 
ISBLANK (Firefox_Release_When_Removed__c))</errorConditionFormula>
        <errorDisplayField>Firefox_Release_When_Removed__c</errorDisplayField>
        <errorMessage>&apos;Firefox Release When Removed&apos; must be filled in when the root certificate status is &apos;Removed&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fill_NSS_Release_when_Root_Cert_Removed</fullName>
        <active>true</active>
        <description>&apos;NSS Release When Removed&apos; must be filled in when the root certificate Status is Removed</description>
        <errorConditionFormula>AND (RecordType.Name = &apos;Root Certificate&apos;, 
ISPICKVAL(Status__c, &apos;Removed&apos;), 
ISBLANK (NSS_Release_When_Removed__c))</errorConditionFormula>
        <errorDisplayField>NSS_Release_When_Removed__c</errorDisplayField>
        <errorMessage>&apos;NSS Release When Removed&apos; must be filled in when the root certificate status is &apos;Removed&apos;</errorMessage>
    </validationRules>
