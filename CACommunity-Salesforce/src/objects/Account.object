        <externalId>false</externalId>
        <formula>IF (AND(Audits_Same_as_Parent__c = FALSE, 
        Standard_Audit__c = NULL,
        BR_Audit__c = NULL,
        EV_Audit__c = NULL), 
    IMAGE(&quot;/resource/Road_Work&quot;, &quot;Road Work&quot;) + &quot;Need Audit Statements&quot;, &quot;&quot;) +

IF (AND( CP_CPS_Same_as_Parent__c  = FALSE, 
         Certificate_Policy_CP_255_char_c__c = NULL,
         Certification_Practice_Stat_CPS_255_char__c = NULL), 
    IMAGE(&quot;/resource/Road_Work&quot;, &quot;Road Work&quot;) + &quot;Need CP/CPS&quot;, &quot;&quot;)</formula>
