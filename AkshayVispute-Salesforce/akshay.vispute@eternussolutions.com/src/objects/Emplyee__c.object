    <validationRules>
        <fullName>Age_Limit_Rule</fullName>
        <active>true</active>
        <description>Age should be between 21-30</description>
        <errorConditionFormula>OR(Age__c &lt;21,Age__c &gt;31)</errorConditionFormula>
        <errorMessage>Age must be between 21-30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DOB_Rule</fullName>
        <active>true</active>
        <description>Employees age should be greater than 18</description>
        <errorConditionFormula>Age__c &lt; 18</errorConditionFormula>
        <errorMessage>Please provide a valid Date of birth. Employee cannot be less than 18 years of Age.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_Working_Day_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>Last_Working_Day_c__c &lt; Date_of_Joining__c</errorConditionFormula>
        <errorMessage>Last working date should be greater than joining date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resignation_Date_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Is_on_Notice_Period__c == True, ISNULL(Resignation_Date__c))</errorConditionFormula>
        <errorMessage>Resignation Date Required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Size_and_Duplicate_Rule_for_SSN</fullName>
        <active>true</active>
        <errorConditionFormula>LEN(TEXT(SSN__c))  &gt;= 10</errorConditionFormula>
        <errorMessage>Maximum size is 10</errorMessage>
    </validationRules>
