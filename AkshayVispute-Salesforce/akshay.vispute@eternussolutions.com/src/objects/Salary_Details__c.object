    <validationRules>
        <fullName>Bank_Account_Details_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNULL(Bank_Account_Number__c),ISNULL(Bank_Details__c) )</errorConditionFormula>
        <errorMessage>Account Details can not be null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Bank_Account_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT( ISNEW() ),
PRIORVALUE(  Bank_Account_Number__c ) &lt;&gt; &quot;&quot;,
ISCHANGED( Bank_Account_Number__c ) 
)</errorConditionFormula>
        <errorMessage>Bank Account number once set cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Inactive_Employee_Rule</fullName>
        <active>true</active>
        <description>Restrict Salary record of inactive employee.</description>
        <errorConditionFormula>AND(
ISNEW(),
Credited_To__r.Is_Active__c = FALSE, Credited_To__r.Is_on_Notice_Period__c = FALSE)</errorConditionFormula>
        <errorMessage>Salary cannot be processed for inactive employees.</errorMessage>
    </validationRules>
